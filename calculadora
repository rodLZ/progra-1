#include <stdio.h>

int main (){
    int num1, num2, resultado;
    printf("dame el primer valor: \n");
    scanf("%d", &num1);
    printf("dame el segundo valor: \n");
    scanf("%d", &num2);
    
    printf("El resultado de la suma:%d \n",num1 + num2);

    printf("El resultado de la resta:%d \n",num1 - num2);

    printf("El resultado de la multi:%d \n",num1 * num2);

    printf("El resultado de la divi:%d \n",num1 / num2);


    return 0;
}
